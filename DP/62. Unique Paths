class Solution {
    public int ans(int i,int j,int[][] dp){
        if(i==0||j==0)
        return 1;
        if(dp[i][j]!=0)
        return dp[i][j];
        dp[i][j] = ans(i,j-1,dp)+ans(i-1,j,dp);
        return dp[i][j];
    }
    public int uniquePaths(int m, int n) {
        int[][] dp = new int[m][n];
        for(int i=0;i<m;i++){
            for(int j=0;j<n;j++)
            dp[i][j] = 0;
        }
        return ans(m-1,n-1,dp);
    }
}
